{
  "name": "importback",
  "version": "1.0.0",
  "description": "importback",
  "license": "ISC",
  "author": "umsehun",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "start" : "uvicorn main:app --reload --port 8000",
    "test": "pytest tests/",
    "test:cov": "pytest tests/ --cov=src --cov-report=term-missing --cov-report=html",
    "type-check": "mypy .",
    "clean": "bash scripts/clean.sh",
    "clear" : "clear clear",
    "clear all" : "find . -type f -name '*.pyc' -delete && find . -type d -name '__pycache__' -delete"
  }
}
